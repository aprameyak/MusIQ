  "description": "MusicBrainz ETL pipeline for MusIQ",
  "main": "dist/scripts/run-musicbrainz-etl.js",
  "scripts": {
    "build": "tsc",
    "etl": "ts-node src/scripts/run-musicbrainz-etl.ts",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
