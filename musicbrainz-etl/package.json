    "axios": "^1.13.2",
    "dotenv": "^16.3.1",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "pg-connection-string": "^2.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  }
